@model IEnumerable<EntityDemo.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>All Students</h2>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>S/N</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    @foreach (var s in Model)
    {
    <tr>
        <td>@s.ID</td>
        <td>@s.stud_name</td>
        <td>@s.email</td>
    </tr>
    }
</table>

